   <extension
         point="org.eclipse.ui.commands">
      <command
            name="Draw Empty Field"
            description="Reset the Field"
            id="com.eco.bio7.empty_field">
      </command>
      <command
            description="Start RServe"
            id="com.eco.bio7.Start_RServe_Action"
            name="Start RServe">
      </command>
      <keyBinding
            commandId="com.eco.bio7.Start_RServe_Action"
            keySequence="CTRL+R"
            keyConfigurationId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </keyBinding>
      <keyBinding
            commandId="com.eco.bio7.empty_field"
            keySequence="CTRL+0"
            keyConfigurationId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </keyBinding>
      <keyBinding
            commandId="com.eco.bio7.Enable_Selection"
            keyConfigurationId="org.eclipse.ui.defaultAcceleratorConfiguration"
            keySequence="ALT+S"/>
      <command
            description="Stop-Action"
            id="com.eco.bio7.StopAction"
            name="Stop Action"/>
      <command
            description="Enable Selection"
            id="com.eco.bio7.Enable_Selection"
            name="Enable Selection"/>
      <command
            description="Stop the flow"
            id="com.eco.bio7.flowstop"
            name="Flowstop"/>
      <command
            description="Evaluate Expression"
            id="com.eco.bio7.print_expression"
            name="Expression Dialog"/>
      <keyBinding
            commandId="com.eco.bio7.flowstop"
            keyConfigurationId="org.eclipse.ui.defaultAcceleratorConfiguration"
            keySequence="CTRL+X"/>
     
      <keyBinding
            commandId="com.eco.bio7.print_expression"
            keyConfigurationId="org.eclipse.ui.defaultAcceleratorConfiguration"
            keySequence="ALT+X"/>
      <command
            description="Executes a line selection in R"
            id="com.eco.bio7.execute_r_select"
            name="R_Selection"/>
      <keyBinding
            commandId="com.eco.bio7.execute_r_select"
            keyConfigurationId="org.eclipse.ui.defaultAcceleratorConfiguration"
            keySequence="F8"/>
            
            <command
            description="Executes a R script"
            id="com.eco.bio7.execute_r_scriptaction"
            name="R_Execute"/>
      <keyBinding
            commandId="com.eco.bio7.execute_r_scriptaction"
            keyConfigurationId="org.eclipse.ui.defaultAcceleratorConfiguration"
            keySequence="F6"/>
            
            
      <command
            id="com.eco.bio7.execute_r_clipboard"
            name="Evaluate R Clipboard">
      </command>
      <keyBinding
            commandId="com.eco.bio7.execute_r_clipboard"
            keyConfigurationId="org.eclipse.ui.defaultAcceleratorConfiguration"
            keySequence="M2+F8">
      </keyBinding>
      <command
            id="com.eco.bio7.execute_r_clipboard_values"
            name="Execute R clipboard values">
      </command>
      <keyBinding
            commandId="com.eco.bio7.execute_r_clipboard_values"
            keyConfigurationId="org.eclipse.ui.defaultAcceleratorConfiguration"
            keySequence="ALT+V">
      </keyBinding>
      <keyBinding
            commandId="com.eco.bio7.activatRPlot"
            keyConfigurationId="org.eclipse.ui.defaultAcceleratorConfiguration"
            keySequence="ALT+A">
      </keyBinding>
      <command
            description="Activate R Plots"
            id="com.eco.bio7.activatRPlot"
            name="Activate R Plots">
      </command>
